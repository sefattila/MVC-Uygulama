@model List<Person>

<table class="table table-light">
    <thead>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Birthdate</th>
            <th>Computer Counts</th>
            <th>Update</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Person person in Model)
        {
            <tr>
                <td>@person.Id</td>
                <td>@person.FirstName</td>
                <td>@person.LastName</td>
                <td>@person.BirthDate</td>
                <td>@person.ComputerCounts</td>
                <td><a class="btn btn-warning" onclick="GetUpdatePage(@person.Id)">Update</a></td>
                <td><a class="btn btn-danger">Delete</a></td>
            </tr>
        }
    </tbody>
</table>